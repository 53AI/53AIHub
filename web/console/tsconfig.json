{
  "compilerOptions": {
    "baseUrl": ".",
    "module": "ESNext",
    "target": "ESNext",
    "lib": [
      "DOM",
      "ESNext"
    ],
    "strict": true,
    "esModuleInterop": true,
    "allowSyntheticDefaultImports": true,
    "jsx": "preserve",
    "moduleResolution": "node",
    "resolveJsonModule": true,
    "noUnusedLocals": true,
    "strictNullChecks": true,
    "forceConsistentCasingInFileNames": true,
    "skipLibCheck": true,
    "paths": {
      "@/*": [
        "./src/*"
      ]
    },
    "types": [
      "vite/client",
      "node",
      "element-plus/global",
      "vue"
    ]
  },
  "exclude": [
    "node_modules",
    "dist",
    "service"
  ],
  "vueCompilerOptions": {
    "target": 3,
    "plugins": [
      "@vue/language-plugin-typescript"
    ]
  }
}
