{
  "extends": "@electron-toolkit/tsconfig/tsconfig.web.json",
  "include": [
    "src/renderer/main/typings/*.d.ts",
    "src/renderer/main/**/*",
    "src/renderer/main/**/*.vue",
    "src/preload/*.d.ts"
  ],
  "compilerOptions": {
    "composite": true,
    "baseUrl": ".",
    "paths": {
      "@/*": [
        "src/renderer/main/*"
      ]
    }
  }
}